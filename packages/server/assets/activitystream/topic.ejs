  href="<%= params.cta.url %>" 
  title="<%= params.cta.altTitle || params.cta.title %>" 
  background-color="white" 
  color="black" 
  font-weight="500" 
  border-radius="5px" 
  border="2px solid rgba(0,76,235,1)"
  >
