<mj-section padding="0">
  <mj-column>
    <mj-button
      background-color="rgba(59, 130, 246, 1)"
      border-radius="8px"
      color="white"
      href="<%- params.cta.url -%>"
      title="<%- params.cta.altTitle || params.cta.title -%>"
      rel="notrack"
      inner-padding="25px 55px"
      text-transform="uppercase"
      font-weight="bold"
    >
      <%- params.cta.title -%>
    </mj-button>
  </mj-column>
</mj-section>
