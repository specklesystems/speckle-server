{
  "name": "@speckle/frontend-2",
  "description": "Speckle frontend build w/ Vue 3 & Nuxt 3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev:app": "concurrently \"nuxt dev\" \"yarn gqlgen:watch\"",
    "dev": "concurrently \"yarn dev:app\" \"yarn gqlgen:watch\" \"yarn storybook --no-open\" -n nuxt,gqlgen,storybook",
    "preview": "nuxt preview",
    "analyze": "nuxt analyze",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".js,.ts,.vue\" .",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:prettier": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --check .",
    "lint:css": "stylelint \"**/*.{css,vue}\"",
    "lint": "yarn lint:js && yarn lint:tsc && yarn lint:prettier && yarn lint:css",
    "gqlgen": "graphql-codegen",
    "gqlgen:watch": "graphql-codegen --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook",
    "storybook:test:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook --ci\"",
    "storybook:test:watch": "test-storybook --watch",
    "chromatic": "chromatic --exit-zero-on-changes --exit-once-uploaded"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@headlessui/vue": "^1.7.3",
    "@heroicons/vue": "^2.0.12",
    "@speckle/shared": "workspace:^",
    "@speckle/viewer": "workspace:^",
    "@speckle/vue-apollo-composable": "^4.0.0-beta.2",
    "@vue/apollo-composable": "4.0.0-beta.1",
    "@vue/apollo-ssr": "^4.0.0-alpha.18",
    "@vueuse/core": "^9.0.0",
    "apollo-upload-client": "^17.0.0",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "mixpanel-browser": "^2.45.0",
    "portal-vue": "^3.0.0",
    "subscriptions-transport-ws": "^0.11.0",
    "vee-validate": "^4.7.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/cli": "^2.13.6",
    "@graphql-codegen/client-preset": "^1.2.5",
    "@nuxtjs/tailwindcss": "^6.2.0",
    "@storybook/addon-a11y": "^7.0.0-alpha.42",
    "@storybook/addon-docs": "^7.0.0-alpha.42",
    "@storybook/addon-essentials": "^7.0.0-alpha.42",
    "@storybook/addon-interactions": "^7.0.0-alpha.42",
    "@storybook/addon-links": "^7.0.0-alpha.42",
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "0.10.0-next.1",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^7.0.0-alpha.42",
    "@storybook/vue3-vite": "^7.0.0-alpha.42",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/vue": "^6.6.1",
    "@types/apollo-upload-client": "^17.0.1",
    "@types/eslint": "^8.4.5",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/mixpanel-browser": "^2.38.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "browserify-zlib": "^0.2.0",
    "chromatic": "^6.11.4",
    "concurrently": "^7.5.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-vue": "^9.5.1",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "jest": "27",
    "nuxt": "^3.0.0",
    "postcss": "^8.4.18",
    "postcss-custom-properties": "^12.1.9",
    "postcss-html": "^1.5.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.7.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "storybook": "^7.0.0-alpha.42",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.2.0",
    "type-fest": "^2.13.1",
    "typescript": "^4.8.3",
    "vue-tsc": "^1.0.8",
    "wait-on": "^6.0.1"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
