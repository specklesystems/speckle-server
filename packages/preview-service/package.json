    "express": "~4.16.1",
    "file-type": "^16.3.0",
    "http-errors": "~1.6.3",
    "knex": "^1.0.3",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "pg-query-stream": "^4.1.0",
    "prom-client": "^14.0.1",
    "puppeteer": "^13.3.2",
    "yargs": "^17.3.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "html-webpack-plugin": "^5.3.1",
